#load "gl.bl"
#import "std/dl"

__gl_get_proc :: fn (proc_name: string) *u8 {
    module, err1 :: dl_open();
    if !is_ok(err1) {
        panic("GL Error: %", err1);
        return null;
    }
    sym, err2 :: dl_sym(module, proc_name);
    if !is_ok(err2) {
        print_err("GL Error: %", err2);
        return null;
    }
    return sym;
}
