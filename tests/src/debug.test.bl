#private
debug_print_log :: fn () #test {
    _context.print_log_fn = &test_logger;
    {
        _expected_kind = PrintLogKind.MESSAGE;
        _expected_file = "";
        _expected_line = 0;
        _expected_format = "";
        _expected_argc = 0; 
        print_log("");
    }

    {
        _expected_kind = PrintLogKind.MESSAGE;
        _expected_file = "";
        _expected_line = 0;
        _expected_format = "hello";
        _expected_argc = 0; 
        print_log("hello");
    }

    {
        _expected_kind = PrintLogKind.MESSAGE;
        _expected_file = "";
        _expected_line = 0;
        _expected_format = "hello";
        _expected_argc = 2; 
        print_log("hello", 10, true);
    }

    _context.print_log_fn = &__print_log_default;
}

