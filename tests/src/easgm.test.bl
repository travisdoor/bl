#private

one :: fn() #test {
	s := S.{ 10, 20, 30 };
	foo(s);
}

S :: struct {
	a: s32;
	b: s32;
	c: s32;
}

foo :: fn (s: S) {
	test_eq(s.a, 10);
	test_eq(s.b, 20);
	test_eq(s.c, 30);
}

